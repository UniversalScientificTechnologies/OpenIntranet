{% extends "base.hbs" %}
{% block title %} UST intranet | Ověření emailu {%end%}
{% block body %}

<style type="text/css">
    body {
        background-color: #f5f5f5;
    }
</style>
<div class="container">
    {% if success %}
    <div class="alert alert-success text-center" role="alert" style="margin-top: 30px">
        Podařilo se, email je ověřen
    </div>
    {% else %}
    <div class="alert alert-danger text-center" role="alert" style="margin-top: 30px">
        Ověření se nezdařilo
    </div>
    {% end %}
</div>

<script type="text/javascript">

</script>

{% end %}
